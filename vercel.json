{
    "functions": {
        "api/index.ts": {
            "includeFiles": "server/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "buildCommand": "vite build",
    "outputDirectory": "dist/public",
    "framework": "vite"
}